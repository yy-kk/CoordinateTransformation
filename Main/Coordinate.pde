public class TwoCoordinate{
  int PointX;
  int PointY;
  public TwoCoordinate(int _x, int _y){
    PointX = _x;
    PointY = _y;
  }
}

public class ThreeCoordinate{
  int PointX;
  int PointY;
  int PointZ;
  public ThreeCoordinate(int _x, int _y, int _z){
    PointX = _x;
    PointY = _y;
    PointZ = _z;
  }
}
